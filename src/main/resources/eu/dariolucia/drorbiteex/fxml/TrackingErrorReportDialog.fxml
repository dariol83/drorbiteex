<AnchorPane maxHeight="1.7976931348623157E308" prefHeight="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.dariolucia.drorbiteex.fxml.TrackingErrorReportDialog">
   <children>
      <VBox spacing="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="periodLabel" text="---" />
            <AreaChart fx:id="azimuthErrorChart" minHeight="300" maxHeight="300" minWidth="800" maxWidth="800">
              <xAxis>
                <NumberAxis fx:id="azimuthErrorTimeAxis" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="azimuthErrorValueAxis" side="LEFT" />
              </yAxis>
            </AreaChart>
            <AreaChart fx:id="elevationErrorChart" minHeight="300" maxHeight="300" minWidth="800" maxWidth="800">
               <xAxis>
                  <NumberAxis fx:id="elevationErrorTimeAxis" side="BOTTOM" />
               </xAxis>
               <yAxis>
                  <NumberAxis fx:id="elevationErrorValueAxis" side="LEFT" />
               </yAxis>
            </AreaChart>
         </children>
      </VBox>
   </children>
</AnchorPane>

<AnchorPane minHeight="0.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.dariolucia.drorbiteex.fxml.progress.ProgressDialog">
   <children>
      <VBox minHeight="0.0" prefWidth="500.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="messageText" maxWidth="1.7976931348623157E308" text="---" VBox.vgrow="NEVER" />
            <ProgressBar fx:id="progress" maxWidth="1.7976931348623157E308" />
            <Label fx:id="ectText" maxWidth="1.7976931348623157E308" text="ECT: ---" />
            <HBox>
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <Button contentDisplay="RIGHT" mnemonicParsing="false" prefWidth="80.0" text="Cancel" onAction="#onCancelButtonAction" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>

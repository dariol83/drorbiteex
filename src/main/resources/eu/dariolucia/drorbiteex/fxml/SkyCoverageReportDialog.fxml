<?import javafx.scene.image.ImageView?>
<AnchorPane maxHeight="1.7976931348623157E308" prefHeight="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.dariolucia.drorbiteex.fxml.SkyCoverageReportDialog">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" spacing="5">
         <children>
            <Label text="---" fx:id="periodLabel" />
            <ImageView fx:id="image" />
            <HBox>
               <Label text="Elevation" minWidth="150" maxWidth="150"/>
               <TextField fx:id="elevationText" editable="false" minWidth="100" maxWidth="100" />
               <Label text="  degrees" />
            </HBox>
            <HBox>
               <Label text="Azimuth" minWidth="150" maxWidth="150"/>
               <TextField fx:id="azimuthText" editable="false" minWidth="100" maxWidth="100" />
               <Label text="  degrees" />
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
